<UserControl x:Class="Slicer.ImageCreator"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Slicer"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"             
             DataContext="{Binding RelativeSource={RelativeSource Self}}"
             MouseMove="UserControl_MouseMove"
             ToolTipService.ShowDuration="32000"
             Loaded="UserControl_Loaded"
             KeyUp="UserControl_KeyUp"
             >
    <UserControl.ToolTip>
        <ToolTip Placement="AbsolutePoint" StaysOpen="True">
            <StackPanel>
                <TextBlock Text="{Binding MouseX, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}"/>
                <TextBlock Text="{Binding MouseY, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}"/>
                <TextBlock Text="{Binding PolarLenght, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}"/>
                <TextBlock Text="{Binding PolarAngle, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}"/>
                
            </StackPanel>
        </ToolTip>
    </UserControl.ToolTip>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="300"/>
        </Grid.ColumnDefinitions>
        <ScrollViewer 
            VerticalScrollBarVisibility="Visible" 
            HorizontalScrollBarVisibility="Visible"
            Name="svImageCreator"
            Grid.Column="0"
            >
            <Grid 
                Width="1530" 
                Height="1530" 
                Name="grdCoord"
                MouseLeftButtonUp="grdCoord_MouseLeftButtonUp"
                IsHitTestVisible="True"
                >
                <Grid.LayoutTransform>
                    <ScaleTransform 
                        ScaleX="{Binding ZoomFactor, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}" 
                        ScaleY="{Binding ZoomFactor, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}"
                        CenterX="765"
                        CenterY="765"
                        />
                </Grid.LayoutTransform>
                <Image Source="{Binding PolarGridBackgroundImage, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}" Stretch="None" VerticalAlignment="Center" HorizontalAlignment="Center"/>

                <!-- Maximum Value, r = 765-->
                <Ellipse Height="1530" Width="1530" StrokeThickness="{Binding StrokeThick, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Stroke="Black" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                <Ellipse Height="1528" Width="1528" StrokeThickness="{Binding StrokeThin, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Stroke="Black" VerticalAlignment="Center" HorizontalAlignment="Center" Visibility="{Binding Class3Visible, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}"/>
